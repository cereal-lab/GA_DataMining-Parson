// The Java program
import java.util.Scanner;
public class Problem
{
public static void main( String args[] )
{
Scanner stdin = new Scanner( System.in );
// Declare month
int month;
// Read month
System.out.print( "Enter the number of the month (1-12)");
month = stdin.nextInt();
// Compute and print the season based on month
if( month  <=  3 )
{
System.out.print( "Season is winter (January-March)");
} // End of if-clause
else
{
if( month  <=  6 )
{
System.out.print( "Season is spring (April-June)");
} // End of if-clause
else
{
if( month  <=  9 )
{
System.out.print( "Season is summer (July-September)");
} // End of if-clause
else
{
System.out.print( "Season is fall (October-December)");
} // End of else-clause
} // End of else-clause
} // End of else-clause
}  // End of method main
} // End of class Problem
