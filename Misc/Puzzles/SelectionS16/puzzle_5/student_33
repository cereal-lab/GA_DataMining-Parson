// The Java program
import java.util.Scanner;
public class Problem
{
public static void main( String args[] )
{
Scanner stdin = new Scanner( System.in );
// Declare numMonth
int numMonth;
// Read numMonth
System.out.print( "Enter the number of the month (1-12)");
numMonth = stdin.nextInt();
// Compute and print the season based on numMonth
if( numMonth  <=  3 )
{
System.out.print( "Season is winter (January-March)");
} // End of if-clause
else
{
if( numMonth  <=  6 )
{
System.out.print( "Season is spring (April-June)");
} // End of if-clause
else
{
if( numMonth  <=  9 )
{
System.out.print( "Season is summer (July-September)");
} // End of if-clause
else
{
System.out.print( "Season is fall (October-December)");
} // End of else-clause
} // End of else-clause
} // End of else-clause
}  // End of method main
} // End of class Problem
