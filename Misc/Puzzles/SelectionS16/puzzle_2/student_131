// The Java program
import java.util.Scanner;
public class Problem
{
public static void main( String args[] )
{
Scanner stdin = new Scanner( System.in );
// Declare numGrade
short numGrade;
// Declare letter
char letter;
// Read numGrade
System.out.print( "Enter the numerical grade");
numGrade = stdin.nextShort();
// Compute letter based on numGrade
if( numGrade  >=  90 )
{
letter  =  'A';
} // End of if-clause
else
{
if( numGrade  >=  80 )
{
letter  =  'B';
} // End of if-clause
else
{
if( numGrade  >=  70 )
{
letter  =  'C';
} // End of if-clause
else
{
if( numGrade  >=  55 )
{
letter  =  'D';
} // End of if-clause
else
{
letter  =  'F';
} // End of else-clause
} // End of else-clause
} // End of else-clause
} // End of else-clause
// Print letter
System.out.print( letter);
}  // End of method main
} // End of class Problem
