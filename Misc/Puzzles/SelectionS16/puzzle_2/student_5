// The Java program
import java.util.Scanner;
public class Problem
{
public static void main( String args[] )
{
Scanner stdin = new Scanner( System.in );
// Declare number
short number;
// Declare letter
char letter;
// Read number
System.out.print( "Enter the numerical grade");
number = stdin.nextShort();
// Compute letter based on number
if( number  >=  90 )
{
letter  =  'A';
} // End of if-clause
else
{
if( number  >=  80 )
{
letter  =  'B';
} // End of if-clause
else
{
if( number  >=  70 )
{
letter  =  'C';
} // End of if-clause
else
{
if( number  >=  55 )
{
letter  =  'D';
} // End of if-clause
else
{
letter  =  'F';
} // End of else-clause
} // End of else-clause
} // End of else-clause
} // End of else-clause
// Print letter
System.out.print( letter);
}  // End of method main
} // End of class Problem
