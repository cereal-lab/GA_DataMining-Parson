// The Java program
import java.util.Scanner;
public class Problem
{
public static void main( String args[] )
{
Scanner stdin = new Scanner( System.in );
// Declare number
short number;
// Declare letterGrade
char letterGrade;
// Read number
System.out.print( "Enter the numerical grade");
number = stdin.nextShort();
// Compute letterGrade based on number
if( number  >=  90 )
{
letterGrade  =  'A';
} // End of if-clause
else
{
if( number  >=  80 )
{
letterGrade  =  'B';
} // End of if-clause
else
{
if( number  >=  70 )
{
letterGrade  =  'C';
} // End of if-clause
else
{
if( number  >=  55 )
{
letterGrade  =  'D';
} // End of if-clause
else
{
letterGrade  =  'F';
} // End of else-clause
} // End of else-clause
} // End of else-clause
} // End of else-clause
// Print letterGrade
System.out.print( letterGrade);
}  // End of method main
} // End of class Problem
