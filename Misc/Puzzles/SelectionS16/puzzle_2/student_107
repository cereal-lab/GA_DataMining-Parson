// The Java program
import java.util.Scanner;
public class Problem
{
public static void main( String args[] )
{
Scanner stdin = new Scanner( System.in );
// Declare numGrade
short numGrade;
// Declare gradeLetter
char gradeLetter;
// Read numGrade
System.out.print( "Enter the numerical grade");
numGrade = stdin.nextShort();
// Compute gradeLetter based on numGrade
if( numGrade  >=  90 )
{
gradeLetter  =  'A';
} // End of if-clause
else
{
if( numGrade  >=  80 )
{
gradeLetter  =  'B';
} // End of if-clause
else
{
if( numGrade  >=  70 )
{
gradeLetter  =  'C';
} // End of if-clause
else
{
if( numGrade  >=  55 )
{
gradeLetter  =  'D';
} // End of if-clause
else
{
gradeLetter  =  'F';
} // End of else-clause
} // End of else-clause
} // End of else-clause
} // End of else-clause
// Print gradeLetter
System.out.print( gradeLetter);
}  // End of method main
} // End of class Problem
