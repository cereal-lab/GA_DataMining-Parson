// The Java program
import java.util.Scanner;
public class Problem
{
public static void main( String args[] )
{
Scanner stdin = new Scanner( System.in );
// Declare windSpeed
int windSpeed;
// Read windSpeed
System.out.print( "Enter the wind speed in miles per hour");
windSpeed = stdin.nextInt();
// Compute and print the Fujita scale corresponding to wind speed
if( windSpeed  >=  261 )
{
System.out.print( "Scale is F5 (over 260 mph)");
} // End of if-clause
else
{
if( windSpeed  >=  207 )
{
System.out.print( "Scale is F4 (207-260 mph)");
} // End of if-clause
else
{
if( windSpeed  >=  158 )
{
System.out.print( "Scale is F3 (158-206 mph)");
} // End of if-clause
else
{
if( windSpeed  >=  113 )
{
System.out.print( "Scale is F2 (113-157 mph)");
} // End of if-clause
else
{
if( windSpeed  >=  73 )
{
System.out.print( "Scale is F1 (73-112 mph)");
} // End of if-clause
else
{
if( windSpeed  >=  40 )
{
System.out.print( "Scale is F0 (40-72 mph)");
} // End of if-clause
else
{
System.out.print( "Not a hurricane");
} // End of else-clause
} // End of else-clause
} // End of else-clause
} // End of else-clause
} // End of else-clause
} // End of else-clause
}  // End of method main
} // End of class Problem
