// The Java program
import java.util.Scanner;
public class Problem
{
public static void main( String args[] )
{
Scanner stdin = new Scanner( System.in );
// Declare waveNM
short waveNM;
// Read waveNM
System.out.print( "Enter the wavelength in nanometers");
waveNM = stdin.nextShort();
// Compute and print the color corresponding to the wavelength
if( waveNM  <=  450 )
{
System.out.print( "Color is violet (380-450 nm)");
} // End of if-clause
else
{
if( waveNM  <=  495 )
{
System.out.print( "Color is blue (450-495 nm)");
} // End of if-clause
else
{
if( waveNM  <=  570 )
{
System.out.print( "Color is green (495-570 nm)");
} // End of if-clause
else
{
if( waveNM  <=  590 )
{
System.out.print( "Color is yellow (570-590 nm)");
} // End of if-clause
else
{
if( waveNM  <=  620 )
{
System.out.print( "Color is orange (590-620 nm)");
} // End of if-clause
else
{
System.out.print( "Color is red (620-750 nm)");
} // End of else-clause
} // End of else-clause
} // End of else-clause
} // End of else-clause
} // End of else-clause
}  // End of method main
} // End of class Problem
