// The Java program
import java.util.Scanner;
public class Problem
{
public static void main( String args[] )
{
Scanner stdin = new Scanner( System.in );
// Declare location
short location;
// Read location
System.out.print( "Enter the location in degrees (1-360)");
location = stdin.nextShort();
// Compute and print the quadrant based on location
if( location  <=  90 )
{
System.out.print( "First quadrant");
} // End of if-clause
else
{
if( location  <=  180 )
{
System.out.print( "Second quadrant");
} // End of if-clause
else
{
if( location  <=  270 )
{
System.out.print( "Third quadrant");
} // End of if-clause
else
{
System.out.print( "Fourth quadrant");
} // End of else-clause
} // End of else-clause
} // End of else-clause
}  // End of method main
} // End of class Problem
