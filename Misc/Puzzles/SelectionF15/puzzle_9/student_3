// The Java program
import java.util.Scanner;
public class Problem
{
public static void main( String args[] )
{
Scanner stdin = new Scanner( System.in );
// Declare sound
short sound;
// Read sound
System.out.print( "Enter the loudness of sound in decibels");
sound = stdin.nextShort();
// Compute and print whether the sound is loud (over 85) or tolerable
if( sound  >  85 )
{
System.out.print( "Sound is loud");
} // End of if-clause
else
{
System.out.print( "Sound is tolerable");
} // End of else-clause
}  // End of method main
} // End of class Problem
